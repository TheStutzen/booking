{
  "name": "booking",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --quiet",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "typeorm_run": "npm run typeorm -- -d src/db/index.ts",
    "migration:generate": "npm run typeorm_run migration:generate ./src/db/migrations/$npm_config_name$_migration",
    "migration:create": "npm run typeorm -- migration:create ./src/db/migrations/$npm_config_name$_migration",
    "migration:run": "npm run typeorm_run migration:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --verbose --config ./test/jest-e2e.json",
    "test:junit": "jest --ci --reporters=default --reporters=jest-junit",
    "test:junit:cov": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "test:e2e:junit": "jest --config ./test/jest-e2e.json --reporters=default --runInBand --ci --reporters=default --reporters=jest-junit",
    "test:e2e:junit:cov": "jest --config ./test/jest-e2e.json --coverage --reporters=default --ci --reporters=default --reporters=jest-junit",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "Maxim Gladkov",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/validator": "^13.15.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "mysql": "^2.18.1",
    "ts-jest": "^29.4.4",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
